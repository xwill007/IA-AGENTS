# Claves de Binance (obligatorio para colocar órdenes reales o en testnet)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# Usar testnet de Binance (recomendado en desarrollo)
BINANCE_TESTNET=true

# Seguridad: solo si estás listo para operar de verdad, cambia a true
TRADING_ENABLED=false

# Parámetros por defecto
DEFAULT_SYMBOL=BTCUSDT
DEFAULT_INTERVAL=1h

# Zona horaria de la app (para timestamps y logs)
TZ=UTC

# Base de datos
DATABASE_URL=postgresql://ia_user:ia_password@postgres:5432/ia_agents
REDIS_URL=redis://redis:6379/0

# Paper Trading
PAPER_TRADING_INITIAL_BALANCE=10000.0
PAPER_TRADING_TRANSACTION_FEE=0.001

# Learning Agent
LEARNING_CONFIDENCE_THRESHOLD=0.6
LEARNING_MIN_TRADES=5

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# n8n
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_n8n_password_here

# Jupyter
JUPYTER_TOKEN=your_jupyter_token_here

# Grafana
GF_SECURITY_ADMIN_PASSWORD=your_grafana_password_here



